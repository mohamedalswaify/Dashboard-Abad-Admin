    <style>
        @@media (min-width: 430px) and (max-width: 991px){
       .items-courses .center-items {
         margin-bottom: 30px;
         width: 100% !important;
        
       }
       }
       .items-courses .center-items{
           width:40%;
       }
        .filter-item{
            background:white;
            border:none;
        }
        .classnone{
            display:none !important
        }
        .classflex{
            display : flex!important
        }
        .imge-bor{
            border-radius:60px
           }

        @@media (min-width: 320px) and (max-width: 991px) {
            .navigation-item {
                color: #0B325C !important;
                padding: 0px;
                border-bottom: 1px solid rgba(0, 0, 0, 0.166);
                text-align: center;
            }

            .dropstart .dropdown-toggle::before {
                transform: rotate(-90deg);
            }

            .dropdown-container li:hover {
                background: transparent;
            }

            .mobile-class {
                padding-right: 10px
            }

            .navigation-link {
                color: #0B325C !important;
                padding: 0 !important;
                padding: 15px 0px !important;
            }

            .dropdown-container {
                position: static !important;
                box-shadow: none !important
            }

            .dropdown-submenu .dropdown-container {
                width: 100% !important;
            }
        }

        .navigation-item {
            display: inline-block;
            position: relative;
            margin-right: 20px;
        }

        .navigation-link {
            text-decoration: none;
            color: white;
            padding: 10px;
            display: block;
            cursor: pointer;
            font-weight: bold
        }

        .dropdown-container {
            display: none;
            position: absolute;
            top: 100%;
            right: 0;
            background-color: #fff;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
            z-index: 1;
        }

        .special-class {
            color: #095f90 !important;
            text-align: right;
            width: auto;
            border-radius: 20px;
            padding-left: 0;
            direction: ltr
        }

            .special-class .dropdown-submenu a {
                color: #095f90 !important;
            }

            .special-class:hover {
                border-radius: 20px;
                outline: none;
            }


        .display-block {
            display: block !important;
        }

        .active .dropdown-container,
        .dropdown-submenu.active .dropdown-container {
            display: block;
        }

        .dropdown-container li {
            border-bottom: 1px solid rgba(0, 0, 0, 0.166);
        }

            .dropdown-container li:last-child {
                border-bottom: none;
            }

            .dropdown-container li:hover {
                background: #eaeced;
            }

        .dropdown-submenu {
            position: relative;
            font-size: 12px;
            font-weight: bold;
            padding: 5px;
            border-bottom: 1px solid #eaeced;
        }

            .dropdown-submenu .dropdown-container {
                display: none;
                top: 0;
                right: 100%;
                width: 300px
            }

            .dropdown-submenu.active .dropdown-container {
                display: none;
            }


        ul {
            list-style: none !important;
        }
          
    </style>
</head>

