    <script>
        var contenetCours;
        var isOnlines;
        var isGoals;

        var filterItems = document.querySelectorAll('.filter-item');
        filterItems.forEach(function (item) {
            item.addEventListener('click', function () {
                var dataCourse = this.getAttribute('data-course');
                contenetCours = dataCourse;
                console.log(contenetCours);
                var allCourseContents = document.querySelectorAll('.courses__content');
                allCourseContents.forEach(function (courseContent) {
                    var isOnline = courseContent.getAttribute('data-onlin');
                     var isGoal = courseContent.getAttribute('data-goal');
