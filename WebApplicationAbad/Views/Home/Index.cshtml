            var img1Elements = this.querySelectorAll('.img1');
            img1Elements.forEach(function(img1Element) {
                img1Element.style.display = 'none';
            });

            var itemsImg = this.querySelector('.items-img');
            itemsImg.style.border = '3px solid #f2b829';

            var itemsWord = this.querySelector('.items-word');
            itemsWord.style.backgroundColor = '#f2b829';
        }

        item.addEventListener('touchstart', handleFilterItem);
        item.addEventListener('click', handleFilterItem);
    });
