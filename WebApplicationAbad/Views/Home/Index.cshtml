            allCourseContents.forEach(function (courseContent) {
                var isOnline = courseContent.getAttribute('data-onlin');
                var dataCourse = courseContent.getAttribute('data-course');
                var isGoal = courseContent.getAttribute('data-goal');
                console.log(contenetCours);
                if (((isChecked && isOnline === '2') || (!isChecked && isOnline === '1')) && (dataCourse == contenetCours && isGoal == isGoals)) {
                    courseContent.style.display = 'flex';
                } else {
                    courseContent.style.display = 'none';
                }
            });
        });

        var goal = document.getElementById('goal');
        goal.addEventListener('change', function () {
            var isChecked = this.checked;
