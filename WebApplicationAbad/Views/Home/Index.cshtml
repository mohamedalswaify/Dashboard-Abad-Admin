  <script>
    var filterItems = document.querySelectorAll('.filter-item');
    filterItems.forEach(function(item) {
        function handleFilterItem() {
            var allImg1Elements = document.querySelectorAll('.img1');
            var allImg2Elements = document.querySelectorAll('.img2');

            allImg2Elements.forEach(function(img2Element) {
                img2Element.style.display = 'none';
            });

            var img2Elements = this.querySelectorAll('.img2');
            img2Elements.forEach(function(img2Element) {
                img2Element.style.display = 'flex';
            });

            filterItems.forEach(function(otherItem) {
                if (otherItem !== item) {
                    var otherImg1Elements = otherItem.querySelectorAll('.img1');
                    otherImg1Elements.forEach(function(otherImg1Element) {
                        otherImg1Element.style.display = 'flex';
                    });
                    var otherItemsImg = otherItem.querySelector('.items-img');
                    otherItemsImg.style.border = '3px solid #095f90';

                    var otherItemsWord = otherItem.querySelector('.items-word');
                    otherItemsWord.style.backgroundColor = '#095f90';
                }
