            var navigationItem = document.getElementById('courses');
            var submenuItems = document.querySelectorAll('.dropdown-submenu');

            navigationItem.addEventListener('click', function (event) {
                if (event.target.classList.contains('dropdown-toggle')) {
                    var dropdown = event.target.nextElementSibling;
                    toggleSubmenu(dropdown);
                }
            });

            function toggleSubmenu(dropdown) {
                if (dropdown.classList.contains('display-block')) {
                    dropdown.classList.remove('display-block');
                    navigationItem.classList.remove('active');
                } else {
                    closeAllSubmenus();
                    dropdown.classList.add('display-block');
                    navigationItem.classList.add('active');
                }
            }

            function closeAllSubmenus() {
                submenuItems.forEach(function (submenu) {
                    var submenuDropdown = submenu.querySelector('.dropdown-container');
                    if (submenuDropdown) {
                        submenuDropdown.classList.remove('display-block');
                        submenu.classList.remove('active'); 
                    }
                });
            }

            submenuItems.forEach(function (submenu) {
                submenu.addEventListener('click', function (event) {
                    event.stopPropagation();
                    var submenuDropdown = submenu.querySelector('.dropdown-container');
                    if (submenuDropdown) {
                        toggleSubmenu(submenuDropdown);
                        submenu.classList.add('active');
                    }
                });
            });

            document.addEventListener('click', function (event) {
                closeAllSubmenus();
            });

            document.addEventListener('click', function (event) {
    if (!event.target.closest('#courses')) {
        closeAllSubmenus();
    }
});

        
</script>

