<script>
     $(document).ready(function () {
        $('input[type="checkbox"]').change(function () {
            filterCourses();
        });

        function filterCourses() {
            var goalFilter = $('#goal').prop('checked');
            var presenceFilter = $('#presence').prop('checked');

            $('.filterable-content').each(function () {
                var goalSupported = $(this).data('goal-supported');
                var presence = $(this).data('presence');

                if ((!goalFilter || goalSupported) && (!presenceFilter || presence === "حضوري")) {
                    $(this).show(); 
                } else {
                    $(this).hide(); 
                }
            });
        }
        filterCourses();
    });
       
</script>



<script>
       var buttons = document.querySelectorAll('.filter-item');
       buttons.forEach(function(button) {
    button.style.backgroundColor = 'red'; 
});
        buttons.forEach(function (button) {
            button.style.backgroundColor = 'transparent';
        });

        function filterCourses() {
            var input, filter, courses, course, i, txtValue;
            input = document.getElementById("searchInput");
            filter = input.value.toUpperCase();
            courses = document.getElementById("al-courses");
            course = courses.getElementsByClassName("courses__content");

            for (i = 0; i < course.length; i++) {
                var headerElement = course[i].getElementsByClassName("course__name")[0];
                txtValue = headerElement ? headerElement.textContent || headerElement.innerText : "";

                if (txtValue.toUpperCase().indexOf(filter) > -1) {
                    course[i].style.display = "";
                } else {
                    course[i].style.display = "none";
                }
            }
        }


        if (window.location.href.indexOf("Home/GetPaymentViews") === -1) {

           var currentURL = window.location.href;

           localStorage.setItem('currentURL', currentURL);

          var storedURL = localStorage.getItem('currentURL');

