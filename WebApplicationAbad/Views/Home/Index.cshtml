@using System.Globalization;
@model IEnumerable<WebApplicationAbad.Areas.AdminCoursesData.Models.CoursesScheduless>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using WebApplicationAbad.Areas.AdminCoursesData.Models;
@using WebApplicationAbad.Data;
@inject ApplicationDbContext context
   
@{
    Layout = null;
    var courserType = context.CoursesTypes.Where(b => !b.IsDelete && !b.IsHide).ToList();
    List<CoursesScheduless> alldata = new List<CoursesScheduless>();
    string vew = string.Empty;
    var Artacle = context.Article.Where(b=>!b.IsDelete).OrderByDescending(b=>b.PublishDate).Take(7).ToList();
    var NowView = context.ViewsNows.FirstOrDefault(b => !b.IsDelete && b.IsOpen);
    if (NowView != null)
    {
        vew = NowView.Head;
    }
    var HomeScreen = context.HomeScreens.Find(1);
    int viewActive = context.ViewsNows.Where(b => b.IsOpen).Count();
}
